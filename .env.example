# PostgreSQL configuration
POSTGRES_CONTAINER_NAME=keycloak_db
POSTGRES_VERSION=16
POSTGRES_RESTART_POLICY=unless-stopped
POSTGRES_DB_NAME=keycloak
POSTGRES_USER=keycloak
POSTGRES_PASSWORD=keycloak_password
POSTGRES_PORT=5432

# PostgreSQL healthcheck configuration
POSTGRES_HEALTHCHECK_INTERVAL=10s
POSTGRES_HEALTHCHECK_TIMEOUT=5s
POSTGRES_HEALTHCHECK_RETRIES=5

# Keycloak configuration
KEYCLOAK_CONTAINER_NAME=keycloak_app
KEYCLOAK_VERSION=23.0.3
KEYCLOAK_RESTART_POLICY=unless-stopped
KEYCLOAK_ADMIN=admin
KEYCLOAK_ADMIN_PASSWORD=admin_password
KEYCLOAK_PORT=8080
KEYCLOAK_COMMAND=start-dev

# Keycloak application configuration
KC_METRICS_ENABLED=true
KC_LOG_LEVEL=INFO

# Keycloak healthcheck configuration
KEYCLOAK_HEALTHCHECK_INTERVAL=30s
KEYCLOAK_HEALTHCHECK_TIMEOUT=10s
KEYCLOAK_HEALTHCHECK_RETRIES=3